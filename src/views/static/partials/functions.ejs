<%
calculateAverage = function(reviews) {
    let overallRating = 0;
    if (reviews.length == 0) {
      overallRating = 0;
    } else {
      let sum = 0;
      reviews.forEach(function(element) {
        sum += element.rating;
        overallRating = sum / reviews.length;
      });
    }
    return overallRating.toFixed(1);
  }
%>

